    "@nestjs/testing": "^7.0.0",
    "@nrwl/angular": "9.6.0",
    "@nrwl/cypress": "9.6.0",
    "@nrwl/jest": "9.6.0",
    "@nrwl/nest": "9.6.0",
    "@nrwl/node": "9.6.0",
    "@nrwl/workspace": "9.6.0",
    "@sparkdesignsystem/spark-angular": "^10.1.3",
    "@sparkdesignsystem/spark": "^13.1.3",
    "@types/aws-lambda": "^8.10.62",
    "@types/aws-serverless-express": "^3.3.3",
    "@types/jest": "25.1.4",
    "@types/node": "~8.9.4",
    "aws-lambda": "^1.0.6",
    "aws-serverless-express": "^3.3.8",
    "codelyzer": "~5.0.1",
    "cypress": "^4.1.0",
